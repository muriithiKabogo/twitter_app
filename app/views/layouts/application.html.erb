<!DOCTYPE html>
<html>
  <head>
    <title> </title>
    <%=render 'layouts/rails-default' %>
    <%= render 'layouts/shim' %>
    
<script type="text/javascript" async="" src="https://d2f7xo8n6nlhxf.cloudfront.net/rakam.min.js"></script>
<!--end rakam-->

  </head>
  <body>
    <%= render 'layouts/header' %>

    <div class="container">
      <% flash.each do |message_type, message| %>
      <div class="alert alert-<%= message_type %>"><%= message %></div>
    <% end %>
    
    <div class="container">
      <%= yield %>
      <%= render 'layouts/footer' %>
      <%= debug(params) if Rails.env.development? %>
    </div>
  </body>
</html>